<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aptmobility.lynx"
    android:installLocation="preferExternal" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission
        android:name="android.permission.READ_EXTERNAL_STORAGE"
        android:maxSdkVersion="18" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.GET_TASKS" />

    <permission
        android:name="com.aptmobility.pushnotification.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <!-- <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <!-- Local Notification -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:name="com.aptmobility.lynx.lynxApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.aptmobility.lynx.RegLogin"
            android:label="@string/title_activity_reg_login2"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="com.aptmobility.phastt.RegLogin" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aptmobility.lynx.PasscodeUnlockActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.PasscodeManagePasswordActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme"
            android:windowSoftInputMode="stateHidden" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.homeScreenActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.settings"
            android:label="@string/title_activity_settings"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.Encounter_EncounterStartActivity"
            android:label="@string/title_activity_encounter__encounter_start"
            android:parentActivityName="com.aptmobility.lynx.homeScreenActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.aptmobility.lynx.homeScreenActivity" />
        </activity>
        <activity
            android:name="com.aptmobility.lynx.encounter_new_partner"
            android:label="@string/title_activity_encounter_new_partner"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.Myscore_Activity"
            android:label="@string/title_activity_myscore_prep"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.Selected_Encounter_Summary"
            android:label="@string/title_activity_encounter__list__summary"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.Selected_Partner_Activity"
            android:label="@string/title_activity_selected__partner__summary"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.Testing_Instructions"
            android:label="@string/title_activity_testing_instructions"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.RequestHomeTestKit"
            android:label="@string/title_activity_request_home_test_kit"
            android:screenOrientation="portrait" >
        </activity>
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.aptmobility.lynx.TestingLocation"
            android:label="@string/title_activity_testing_location"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.aptmobility.lynx.testing_nearestTestingLocation"
            android:label="@string/title_activity_testing_nearest_testing_location"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.aptmobility.lynx.homeScreenActivity" />
        </activity>
        <activity
            android:name="com.aptmobility.lynx.PasscodeChangePasscode"
            android:label="@string/title_activity_passcode_change_passcode"
            android:parentActivityName="com.aptmobility.lynx.PasscodeUnlockActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.aptmobility.lynx.PasscodeUnlockActivity" />
        </activity>

        <receiver android:name="com.aptmobility.lynx.AlarmReceiver" >
        </receiver>
        <receiver android:name="com.aptmobility.lynx.AlarmRestoreOnBoot" >
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
        </receiver>

        <activity
            android:name="com.aptmobility.lynx.ConnectingToCare"
            android:label="@string/title_activity_connecting_to_care"
            android:screenOrientation="portrait" >
        </activity>

        <receiver
            android:name="com.google.android.gms.gcm.GcmReceiver"
            android:exported="true"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />

                <category android:name="com.example.gcm" />
            </intent-filter>
        </receiver>

        <service
            android:name="com.aptmobility.lynx.MyGcmListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            </intent-filter>
        </service>
        <service
            android:name="com.aptmobility.lynx.MyInstanceIDListenerService"
            android:exported="false" >
            <intent-filter>
                <action android:name="com.google.android.gms.iid.InstanceID" />
            </intent-filter>
        </service>
        <service
            android:name="com.aptmobility.lynx.RegistrationIntentService"
            android:exported="false" />
        <service android:name=".FirbaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT" />
            </intent-filter>
        </service>
        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>
