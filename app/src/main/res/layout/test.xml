    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/regSumm_partner_haveOtherPartner">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Primary partner has other partners :"
        android:textColor="@color/white"
        android:id="@+id/otherPartner"
        android:textSize="18sp"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes or No"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:textSize="@dimen/reg_tv_textsize"
        android:id="@+id/regSumm_otherPripartner"/>
</LinearLayout>

<View
    android:layout_width="match_parent"
    android:layout_height="2dp"
    android:background="@color/edittext_bg"
    android:layout_marginTop="@dimen/activity_vertical_margin" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/regSumm_partner_addToPartnerlist">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add primary partner to Sex Pro :"
        android:textColor="@color/white"
        android:id="@+id/sexproList"
        android:textSize="18sp"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Yes or No"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:textSize="@dimen/reg_tv_textsize"
        android:id="@+id/regSumm_BBlist"/>
</LinearLayout>

<View
    android:layout_width="match_parent"
    android:layout_height="2dp"
    android:background="@color/edittext_bg"
    android:layout_marginTop="@dimen/activity_vertical_margin" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/regSumm_drug_container_title">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="In the last three months, you used :"
        android:textColor="@color/white"
        android:id="@+id/drugTitle"
        android:textSize="18sp"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:id="@+id/regSumm_drug_container"
        android:orientation="vertical">
    </LinearLayout>
</LinearLayout>

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:visibility="gone"
    android:id="@+id/regSumm_noofdays_alcoholUse">
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/edittext_bg"
        android:layout_marginTop="@dimen/activity_vertical_margin" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:id="@+id/alcholtitle"
        android:text="In the last three months,number of days per week you typically drank alcohol:" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="x"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:textSize="@dimen/reg_tv_textsize"
        android:id="@+id/regSumm_daysCount"/>
</LinearLayout>
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:visibility="gone"
    android:id="@+id/regSumm_noofDrinks">
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@color/edittext_bg"
        android:layout_marginTop="@dimen/activity_vertical_margin" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:id="@+id/drinksCount"
        android:text="Number of drinks you had on a typical day:" />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/border_full_white"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:orientation="horizontal">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="x"
            android:ems="4"
            android:padding="10dp"
            android:background="@android:color/transparent"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/activity_horizontal_margin"
            android:layout_marginRight="@dimen/activity_horizontal_margin"
            android:textSize="@dimen/reg_tv_textsize"
            android:id="@+id/regSumm_drinksCount"/>
    </LinearLayout>
</LinearLayout>

<View
    android:layout_width="match_parent"
    android:layout_height="2dp"
    android:background="@color/edittext_bg"
    android:layout_marginTop="@dimen/activity_vertical_margin" />
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:id="@+id/regSumm_diag_container_title">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="In the last three months, you were diagnosed with:"
        android:textColor="@color/white"
        android:id="@+id/diagTitle"
        android:textSize="18sp"
        android:layout_marginTop="@dimen/activity_vertical_margin"/>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/activity_vertical_margin"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:id="@+id/regSumm_diag_container"
        android:orientation="vertical">
    </LinearLayout>
</LinearLayout>

