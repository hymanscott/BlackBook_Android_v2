                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:padding="10dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@drawable/toggle_button"/>
                    <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/toggle_btn_height"
                        android:textOn="I got fucked"
                        android:textOff="I got fucked"
                        android:textColor="@color/colorPrimary"
                        android:id="@+id/sexType_gotFucked"
                        android:layout_gravity="center"
                        android:textAlignment="gravity"
                        android:enabled="false"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:padding="10dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@drawable/toggle_button"/>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="32dp">

                    <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/toggle_btn_height"
                        android:textOn="I rimmed"
                        android:textColor="@color/colorPrimary"
                        android:textOff="I rimmed"
                        android:layout_gravity="center"
                        android:id="@+id/sexType_iRimmed"
                        android:textAlignment="gravity"
                        android:enabled="false"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:padding="10dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@drawable/toggle_button"/>
                    <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/toggle_btn_height"
                        android:textOn="I got rimmed"
                        android:textOff="I got rimmed"
                        android:textColor="@color/colorPrimary"
                        android:id="@+id/sexType_gotRimmed"
                        android:layout_gravity="center"
                        android:textAlignment="gravity"
                        android:enabled="false"
                        android:layout_margin="10dp"
                        android:layout_weight="1"
                        android:textSize="12sp"
                        android:padding="10dp"
                        android:gravity="center_horizontal|center_vertical"
                        android:background="@drawable/toggle_button"/>

                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="40dp"
                    android:paddingTop="10dp"
                    android:paddingBottom="10dp"
                    android:paddingLeft="75dp"
                    android:paddingRight="75dp"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    android:background="@drawable/border_full_white"
                    android:text="EDIT"
                    android:onClick="editGroupEncounterSummary"
                    android:id="@+id/edit_grp_details"/>
                <Button
                    android:id="@+id/group_sex_confirm"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_alignParentBottom="true"
                    android:layout_marginTop="@dimen/button_top_margin"
                    android:background="@color/encounter_btn_bg"
                    android:onClick="onGroupSexConfirm"
                    android:text="Confirm"
                    android:textColor="@color/white"
                    android:textSize="@dimen/button_textsize"
                    android:textStyle="bold" />
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>

</LinearLayout>