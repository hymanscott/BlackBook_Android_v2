                    android:id="@+id/typeOfSex"/>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="32dp">

                    <ToggleButton
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/toggle_btn_height"
