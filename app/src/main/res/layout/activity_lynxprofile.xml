<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:fillViewport="false">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:text="Update Profile"
                android:layout_width="fill_parent"
                android:textAllCaps="true"
                android:layout_height="wrap_content"
                android:textColor="@color/blue_theme"
                android:textSize="24sp"
                android:layout_margin="10dp"
                android:gravity="center_horizontal|center_vertical" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <com.aptmobility.lynx.CustomTextView
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Name:"
                    android:textSize="20sp"
                    android:textColor="@color/blue_theme"
                    android:layout_margin="10dp"
                    android:gravity="center_vertical" />
                <com.aptmobility.lynx.CustomTextView
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Name"
                    android:id="@+id/profile_name"
                    android:layout_margin="10dp"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:layout_weight="1"/>
            </LinearLayout>

            <com.aptmobility.lynx.CustomTextView
                android:id="@+id/profile_email"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:layout_weight="1"
                android:text="Email"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:textColor="@color/text_color"
                android:textSize="18sp"
                custom:fontAssetName="RobotoSlabRegular.ttf" />

            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Password"
                android:textSize="20sp"
                android:textColor="@color/blue_theme"
                android:layout_marginTop="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" >

                <com.aptmobility.lynx.CustomEditText
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:id="@+id/updatePass"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:hint="Password"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:background="@drawable/apptheme_edit_text_holo_light"
                    android:inputType="textPassword"
                    android:maxLength="12"/>

                <com.aptmobility.lynx.CustomEditText
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:id="@+id/updateRepPass"
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:background="@drawable/apptheme_edit_text_holo_light"
                    android:hint="Repeat Password"
                    android:inputType="textPassword"
                    android:maxLength="12"/>

            </LinearLayout>

            <com.aptmobility.lynx.CustomEditText
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/updatePhone"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:hint="Mobile Phone Number"
                android:textSize="18sp"
                android:textColor="@color/text_color"
                android:layout_margin="5dp"
                android:background="@drawable/apptheme_edit_text_holo_light"
                android:inputType="phone"
                android:maxLength="11"/>

            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Passcode"
                android:layout_marginTop="10dp"
                android:layout_marginRight="5dp"
                android:textSize="20sp"
                android:textColor="@color/blue_theme"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <com.aptmobility.lynx.CustomEditText
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:ems="10"
                android:id="@+id/updatePasscode"
                android:layout_weight="1"
                android:layout_margin="5dp"
                android:background="@drawable/apptheme_edit_text_holo_light"
                android:textSize="18sp"
                android:textColor="@color/text_color"
                android:hint="4 Digit Passcode"
                android:inputType="numberPassword"
                android:maxLength="4"/>

            <Spinner
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/updateSecQuestion"
                style="@style/SpinnerAppTheme"
                android:prompt="@string/sec_question_prompt"
                android:theme="@style/AppTheme"
                android:layout_margin="5dp" />

            <com.aptmobility.lynx.CustomEditText
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:background="@drawable/apptheme_edit_text_holo_light"
                android:id="@+id/updateSecAnswer"
                android:hint="Your answer to the Securtiy Question"
                android:textSize="18sp"
                android:textColor="@color/text_color"
                android:inputType="text"
                android:layout_alignParentBottom="true"
                android:layout_centerHorizontal="true"
                android:layout_margin="5dp" />

            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Demographics"
                android:layout_marginTop="10dp"
                android:textSize="20sp"
                android:textColor="@color/blue_theme"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <com.aptmobility.lynx.CustomTextView
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceSmall"
                    android:text="Date of Birth"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:layout_margin="5dp" />

                <com.aptmobility.lynx.CustomEditText
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:focusable="true"
                    android:background="@drawable/apptheme_edit_text_holo_light"
                    android:ems="6"
                    android:id="@+id/updateDOB"
                    android:layout_margin="5dp"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:hint="MM/DD/YYYY"
                    android:inputType="date" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:scaleType="fitXY"
                    android:id="@+id/calenderIconUpdateDOB"
                    android:src="@drawable/calender"
                    android:layout_margin="5dp"
                    android:layout_gravity="center"/>

            </LinearLayout>
            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="Race/Ethnicity"
                android:textSize="20sp"
                android:textColor="@color/blue_theme"
                android:id="@+id/textView4"
                android:layout_marginTop="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold"
                android:layout_weight="1" />
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true">

                <com.aptmobility.lynx.MultiSelectionSpinner
                    android:id="@+id/mySpinner"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/apptheme_edit_text_holo_light"
                    android:layout_margin="5dp"/>

            </LinearLayout>

            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceMedium"
                android:text="PrEP Status"
                android:layout_marginTop="10dp"
                android:textSize="20sp"
                android:textColor="@color/blue_theme"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp"
                android:layout_marginLeft="5dp"
                android:textStyle="bold"
                android:layout_weight="1" />

            <com.aptmobility.lynx.CustomTextView
                custom:fontAssetName="RobotoSlabRegular.ttf"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:text="I'm taking Pre-exposure Prophylaxis:"
                android:textSize="20sp"
                android:textColor="@color/text_color"
                android:layout_margin="5dp" />

            <RadioGroup
                android:id="@+id/updatePrep"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="5dp">

                <com.aptmobility.lynx.CustomRadioButton
                    style="@style/Phastt.Radiobutton"
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Yes"
                    android:id="@+id/updatePrepYes"
                    />

                <com.aptmobility.lynx.CustomRadioButton
                    style="@style/Phastt.Radiobutton"
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="No"
                    android:textSize="18sp"
                    android:textColor="@color/text_color"
                    android:id="@+id/updatePrepNo"
                    />
            </RadioGroup>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/border_top_bottom"
                android:paddingBottom="10dp"
                android:layout_marginTop="10dp"
                android:paddingTop="10dp"
                android:id="@+id/LynxRemainerLayout">
                <com.aptmobility.lynx.CustomTextView
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:text="LYNX Reminder"
                    android:layout_width="fill_parent"
                    android:textAllCaps="true"
                    android:layout_height="wrap_content"
                    android:textColor="@color/blue_theme"
                    android:textSize="24sp"
                    android:layout_margin="10dp"
                    android:gravity="center_horizontal|center_vertical" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Set Day of the Week:"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical" />
                    <Spinner
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        style="@style/SpinnerAppTheme"
                        android:layout_marginLeft="5dp"
                        android:id="@+id/spinner_druguse_history_day"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="10dp"
                    android:orientation="horizontal">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:text="Set Time of the Day:"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical" />
                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:background="@drawable/apptheme_edit_text_holo_light"
                        android:ems="6"
                        android:clickable="true"
                        android:text="10:00 AM"
                        android:id="@+id/lynxRemainderTime"
                        android:layout_marginLeft="5dp"
                        android:textSize="18sp"
                        android:textColor="@color/text_color"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginBottom="20dp"
                    android:orientation="vertical">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Set Notification Text:"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical" />
                    <com.aptmobility.lynx.CustomEditText
                        android:background="@drawable/apptheme_edit_text_holo_light"
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textMultiLine"
                        android:ems="10"
                        android:layout_marginRight="10dp"
                        android:layout_marginLeft="10dp"
                        android:textSize="18sp"
                        android:layout_gravity="center_horizontal"
                        android:textColor="@color/text_color"
                        android:id="@+id/sexDruguseHistory_notes"
                        android:layout_centerHorizontal="true"
                        android:hint="Do you have anything to report this week?"
                        android:text="Do you have anything to report this week?"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:background="@drawable/border_bottom"
                android:paddingBottom="20dp"
                android:id="@+id/LynxTestRemainerLayout">
                <com.aptmobility.lynx.CustomTextView
                    custom:fontAssetName="RobotoSlabRegular.ttf"
                    android:text="Testing Reminder"
                    android:layout_width="fill_parent"
                    android:textAllCaps="true"
                    android:layout_height="wrap_content"
                    android:textColor="@color/blue_theme"
                    android:textSize="24sp"
                    android:layout_margin="10dp"
                    android:gravity="center_horizontal|center_vertical" />
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="horizontal">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Set Day of the Week:"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical" />
                    <Spinner
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/spinner_remindertest_day"
                        style="@style/SpinnerAppTheme"
                        android:gravity="center_horizontal"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="10dp"
                    android:orientation="horizontal">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:text="Set Time of the Day:"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="10dp"
                        android:gravity="center_vertical" />

                    <com.aptmobility.lynx.CustomTextView
                        android:id="@+id/lynxTestingTime"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_weight="1"
                        android:background="@drawable/apptheme_edit_text_holo_light"
                        android:clickable="true"
                        android:ems="6"
                        android:text="10:00 AM"
                        android:textAppearance="?android:attr/textAppearanceMedium"
                        android:textColor="@color/text_color"
                        android:textSize="18sp"
                        custom:fontAssetName="RobotoSlabRegular.ttf" />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="20dp"
                    android:orientation="vertical">

                    <com.aptmobility.lynx.CustomTextView
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:textSize="20sp"
                        android:textColor="@color/text_color"
                        android:text="Set Notification Text:"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="20dp"
                        android:gravity="center_vertical" />
                    <com.aptmobility.lynx.CustomEditText
                        android:background="@drawable/apptheme_edit_text_holo_light"
                        custom:fontAssetName="RobotoSlabRegular.ttf"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textMultiLine"
                        android:ems="10"
                        android:textSize="18sp"
                        android:textColor="@color/text_color"
                        android:layout_marginLeft="10dp"
                        android:layout_marginRight="10dp"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:id="@+id/reminderTest_notes"
                        android:hint="It's time for your test!"
                        android:text="It's time for your test!"/>

                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="10dp"
                android:paddingTop="10dp"
                android:id="@+id/partnerRatingsLayout">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <com.aptmobility.lynx.CustomTextView
                        android:id="@+id/customTextView"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Configure Partner Ratings"
                        android:textAllCaps="true"
                        android:gravity="center_horizontal"
                        android:layout_alignParentLeft="true"
                        android:textColor="@color/blue_theme"
                        android:textSize="24sp"
                        custom:fontAssetName="RobotoSlabRegular.ttf" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/notification"
                        android:layout_alignParentRight="true"
                        android:id="@+id/imgBtn_partner_ratings"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="10dp"
                        android:layout_gravity="center"/>
                </RelativeLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                        <com.aptmobility.lynx.CustomTextView
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:text="Ratings Category 1:"
                            android:textSize="20sp"
                            android:textColor="@color/text_color"
                            android:layout_width="wrap_content"
                            android:layout_marginBottom="10dp"
                            android:layout_height="wrap_content"

                            />
                        <com.aptmobility.lynx.CustomTextView
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:id="@+id/txt_overall_rtg"
                            android:layout_marginBottom="10dp"
                            android:textSize="18sp"
                            android:textColor="@color/text_color"
                            android:layout_marginLeft="10dp"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:text="Overall"/>

                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                        <com.aptmobility.lynx.CustomTextView
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:text="Ratings Category 2:"
                            android:layout_width="wrap_content"
                            android:textSize="20sp"
                            android:textColor="@color/text_color"
                            android:layout_height="wrap_content"
                            />
                        <com.aptmobility.lynx.CustomEditText
                            android:background="@drawable/apptheme_edit_text_holo_light"
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:layout_width="match_parent"
                            android:textSize="18sp"
                            android:textColor="@color/text_color"
                            android:layout_height="wrap_content"
                            android:id="@+id/rating_ctg2"
                            android:hint="Chemistry"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                        <com.aptmobility.lynx.CustomTextView
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:text="Ratings Category 3:"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="20sp"
                            android:textColor="@color/text_color"
                            />
                        <com.aptmobility.lynx.CustomEditText
                            android:background="@drawable/apptheme_edit_text_holo_light"
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:textColor="@color/text_color"
                            android:id="@+id/rating_ctg3"
                            android:hint="Personality"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content">
                        <com.aptmobility.lynx.CustomTextView
                            custom:fontAssetName="RobotoSlabRegular.ttf"
                            android:text="Ratings Category 4:"
                            android:layout_width="wrap_content"
                            android:textSize="20sp"
                            android:textColor="@color/text_color"
                            android:layout_height="wrap_content"
                            />
                        <com.aptmobility.lynx.CustomEditText
